{
  "name": "theatre-front",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint-css": "stylelint --config .stylelintrc 'src/**/*.scss' --fix",
    "e2e": "ng e2e",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "build:uk-ua": "ng build --prod --aot true --vendor-chunk true --i18n-file src/assets/locale/locale.uk-ua.xlf --i18n-format xlf --i18n-locale uk-UA --output-path 'dist/uk-UA/' --verbose --i18n-missing-translation=error",
    "build:en-us": "ng build --prod --aot true --vendor-chunk true --output-path 'dist/en-US/'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.13",
    "@angular/common": "^9.1.13",
    "@angular/compiler": "^9.1.13",
    "@angular/core": "^9.1.13",
    "@angular/forms": "^9.1.13",
    "@angular/localize": "^9.1.13",
    "@angular/platform-browser": "^9.1.13",
    "@angular/platform-browser-dynamic": "^9.1.13",
    "@angular/router": "^9.1.13",
    "@kolkov/ngx-gallery": "^1.2.3",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "angular-calendar": "^0.27.21",
    "bootstrap": "^4.3.1",
    "class-transformer": "^0.2.3",
    "core-js": "^3.1.4",
    "date-fns": "^2.0.0-beta.3",
    "ngx-infinite-scroll": "^7.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "rxjs-compat": "^6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.13",
    "@angular/cli": "^9.1.13",
    "@angular/compiler-cli": "^9.1.13",
    "@angular/language-service": "^9.1.13",
    "@types/jasmine": "~3.3.16",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.3",
    "stylelint": "^10.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.9.1",
    "typescript": "3.8.3",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
